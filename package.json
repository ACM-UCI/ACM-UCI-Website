{
    "name": "acm-reactify",
    "version": "0.1.0",
    "private": true,
    "homepage": "http://clubs.uci.edu/acm",
    "dependencies": {
        "@devexpress/dx-react-core": "^1.10.3",
        "@devexpress/dx-react-grid": "^1.10.3",
        "@devexpress/dx-react-grid-bootstrap3": "^1.10.3",
        "@devexpress/dx-react-grid-bootstrap4": "^1.10.3",
        "@devexpress/dx-react-grid-material-ui": "^1.10.3",
        "@material-ui/core": "^3.9.2",
        "@material-ui/icons": "^3.0.2",
        "babel-plugin-prismjs": "^1.0.2",
        "bootstrap": "^4.3.1",
        "classnames": "^2.2.6",
        "code": "^5.2.4",
        "firebase": "^5.8.1",
        "googleapis": "^36.0.0",
        "hoek": "^4.2.1",
        "jquery": "^3.3.1",
        "konami": "^1.6.2",
        "node-sass": "^4.9.2",
        "react": "^16.7.0",
        "react-bootstrap": "^1.0.0-beta.5",
        "react-dom": "^16.4.1",
        "react-konami-code": "^1.4.0",
        "react-md": "^1.9.3",
        "react-profiles": "^0.1.4",
        "react-responsive-carousel": "^3.1.49",
        "react-router": "^4.3.1",
        "react-router-dom": "^4.3.1",
        "react-scripts": "^2.1.8",
        "react-syntax-highlighter": "^8.1.0",
        "reactstrap": "^6.3.0",
        "readline": "^1.3.0",
        "vertical-timeline-component-for-react": "^1.0.6"
    },
    "scripts": {
        "build-css": "node-sass --include-path ./src/scss --include-path ./node-modules src/scss/ -o src/scss/",
        "watch-css": "npm run build-css && node-sass --include-path ./src/scss --include-path ./node-modules src/scss/ -o src/scss/ --watch --recursive",
        "start-js": "react-scripts start",
        "start": "npm-run-all -p watch-css start-js",
        "build": "npm run build-css && react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject",
        "format": "prettier --write '**/*.{js,jsx}'"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx,json}": [
            "prettier --write",
            "git add"
        ]
    },
    "devDependencies": {
        "husky": "^1.3.1",
        "lint-staged": "^8.1.0",
        "npm-run-all": "^4.1.3",
        "prettier": "^1.15.3",
        "react-social-icons": "^4.1.0"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
