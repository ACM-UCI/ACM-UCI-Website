{
  "name": "acm-reactify",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://clubs.uci.edu/acm",
  "dependencies": {
    "bootstrap": "^4.1.3",
    "classnames": "^2.2.6",
    "node-sass": "^4.9.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "react-syntax-highlighter": "^8.0.1",
    "reactstrap": "^6.3.0"
  },
  "scripts": {
    "build-css": "node-sass --include-path ./src/scss --include-path ./node-modules src/scss/ -o src/scss/",
    "watch-css": "npm run build-css && node-sass --include-path ./src/scss --include-path ./node-modules src/scss/ -o src/scss/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.3"
  }
}
